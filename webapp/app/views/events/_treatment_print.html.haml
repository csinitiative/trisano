- core_element_print :treatment_id, f, :vert do
  %span.print-label= ct('treatment_name')
  %span.print-value= h(f.object.safe_call_chain(:treatment, :treatment_name))
- core_element_print :treatment_date, f, :vert do
  %span.print-label= ct('treatment_date')
  %span.print-value= h(f.object.safe_call_chain(:treatment_date))
- core_element_print :stop_treatment_date, f, :vert do
  %span.print-label= ct 'stop_treatment_date'
  %span.print-value= h(f.object.safe_call_chain(:stop_treatment_date))
