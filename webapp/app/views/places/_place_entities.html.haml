%table#entity_search_results{:cellpadding => 0, :cellspacing => 0, :border => 0 }
  %tr{:style => 'text-align: left'}
    %th=t 'place_name'
    %th=t 'address'
    %th=t 'place_type'
    %th=t 'actions'
  -place_entities.each do |place_entity|
    %tr.search-active.tabular
      %td=h place_entity.place.name
      %td=render_place_address(place_entity)
      %td=render_place_types(place_entity)
      %td=render_place_actions(place_entity)
