<h1>Listing Confidential Morbidity Reports</h1>

<table>
<% for event in @events %>
  <tr>
    <td><%= event.event_name %>, <%= event.event_onset_date %> <%= l event.event_status %> </td>
    <td><%= link_to 'Show', cmr_path(event) %></td>
    <td><%= link_to 'Edit', edit_cmr_path(event) %></td>
    <td><%= link_to 'Destroy', cmr_path(event), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Morbidity Report', new_cmr_path %>
