%span.section-header Reporting Information
%br

- entity = @event.reporter.active_secondary_entity

#reporter
  .three-valued-row
    .long
      %span.print-label
        Name of reporter:
      %span.print-value
        = entity.person.first_name unless @event.reporter.nil?
        = entity.person.last_name unless @event.reporter.nil?
    .long
      %span.print-label
        Reporting agency:
      %span.print-value
        = @event.reporting_agency.active_secondary_entity.place.name unless @event.reporting_agency.nil?

  .three-valued-row
    %hr
    %br
    .double
      %span.print-label
        Reporter phone number:
      %span.print-value
        - p = entity.telephone unless @event.reporter.nil?
        - if not p.nil?
          = fml " (", p.area_code, ") " if p.area_code
          = phone_number(p.phone_number)
          = fml " Ext. ", p.extension, "" if p.extension

  .three-valued-row
    %hr
    %br
    .long
      %span.print-label
        Results reported to Clinician:
      %span.print-value
        = @event.results_reported_to_clinician_date 
    .long
      %span.print-label
        First reported to public health:
      %span.print-value
        = @event.first_reported_PH_date
