%h1 Editing Answer Set

= error_messages_for :answer_set

- form_for(@answer_set) do |f|

  = render :partial => 'form', :locals => { :f => f }
  
  %p
    %span{:id => "submitbutton" }
      %span{:class => "first-child"}
        = f.submit "Update"


= link_to 'Show', @answer_set
|
= link_to 'Back', answer_sets_path 

%br

<script type="text/javascript">
var oButton = new YAHOO.widget.Button("submitbutton", { type: "submit" } );
</script>